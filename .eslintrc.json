{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "no-use-before-define": ["error", { "functions": true, "classes": false, "variables": false }],
    "no-nested-ternary": 0,
    "no-console": ["error", { "allow": ["error"] }],
    "valid-typeof": [ "error", { "requireStringLiterals": false }]
  }
}
